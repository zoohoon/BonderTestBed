            <GroupBox Grid.Row="3" BorderBrush="White" BorderThickness="1" Margin="1" FontSize="15" Foreground="White" Header="FD Chuck Jog" Grid.ColumnSpan="2">
                <GroupBox.Style>
                    <Style BasedOn="{x:Null}"/>
                </GroupBox.Style>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1.2*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Margin="10,0,10,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="3*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Label Content="FD Chuck Rotation :" FontSize="13" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,-60,0"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Label Content="FD Chuck Z :" FontSize="13" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,-20,0"/>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Label Content="Ejection Base X :" FontSize="13" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,-30,0"/>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Label Content="Ejection Base Y :" FontSize="13" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,-30,0"/>
                            </Grid>
                            <Grid Grid.Row="4">
                                <Label Content="Ejection Base Z :" FontSize="13" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,-30,0"/>
                            </Grid>
                            <Grid Grid.Row="5">
                                <Label Content="Ejection Pin Z  :" FontSize="13" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,-30,0"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
