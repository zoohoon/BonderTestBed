<UserControl x:Class="ManualJogView.ManualJogPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ManualJogView"
             xmlns:vc="clr-namespace:ValueConverters;assembly=ValueConverters"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"             
             xmlns:cui="clr-namespace:CUI;assembly=CUI"
             mc:Ignorable="d" 
             d:DesignHeight="3000" d:DesignWidth="2560"
>
    <UserControl.Resources>
        <ResourceDictionary>
            <vc:WaferOnCSTColConverter x:Key="WaferToColConv"/>
            <vc:HomeToggleColorCon x:Key="ToggleColorCon"/>
            <vc:WaferOnCSTButtonConverter x:Key="WaferOnCSTButtonCon"/>

        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="101*"/>
                <ColumnDefinition Width="280*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="250*"/>
                <RowDefinition Height="250*"/>
                <RowDefinition Height="250*"/>
                <RowDefinition Height="250*"/>
                <RowDefinition Height="250*"/>
                <RowDefinition Height="250*"/>
                <RowDefinition Height="250*"/>
            </Grid.RowDefinitions>
            <GroupBox Grid.Row="0" BorderBrush="White" BorderThickness="1" Margin="1,1,1,1" FontSize="15" Foreground="White" Header="Opitcs Jog (1 CAM ~ 2 CAM)" Grid.ColumnSpan="2">
                <GroupBox.Style>
                    <Style BasedOn="{x:Null}"/>
                </GroupBox.Style>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1.2*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Margin="10,0,10,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="3*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Label Content="1 CAM X :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Label Content="1 CAM Y :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Label Content="1 CAM Z :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Label Content="2 CAM X :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="4">
                                <Label Content="2 CAM Y :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="5">
                                <Label Content="2 CAM Z :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Label Grid.Row="0" 
                                   Width="130" 
                                   HorizontalAlignment="Right" VerticalAlignment="Stretch" 
                                   Background="Black" Foreground="White" 
                                   FontSize="14" 
                                   Content="{Binding CX1.Status.RawPosition.Actual}" 
                                   ContentStringFormat="F2"
                                   />
                            <Label Grid.Row="1"
                                   Width="130" 
                                   HorizontalAlignment="Right" VerticalAlignment="Stretch" 
                                   Foreground="White" 
                                   FontSize="14"
                                   Content="{Binding CY1.Status.RawPosition.Actual}" 
                                   ContentStringFormat="F2"
                                   />
                            <Label Grid.Row="2"
                                   Width="130" 
                                   HorizontalAlignment="Right" VerticalAlignment="Stretch" 
                                   Foreground="White" 
                                   FontSize="14"
                                   Content="{Binding CZ1Axis.Status.RawPosition.Actual}" 
                                   ContentStringFormat="F2"
                                   />
                            <Label Grid.Row="3" 
                                   Width="130" 
                                   HorizontalAlignment="Right" VerticalAlignment="Stretch" 
                                   Foreground="White" 
                                   FontSize="14" 
                                   Content="{Binding CX2Axis.Status.RawPosition.Actual}" 
                                   ContentStringFormat="F2"
                                   />
                            <Label Grid.Row="4"
                                   Width="130" 
                                   HorizontalAlignment="Right" VerticalAlignment="Stretch"
                                   Foreground="White" 
                                   FontSize="14"
                                   Content="{Binding CY2Axis.Status.RawPosition.Actual}" 
                                   ContentStringFormat="F2"
                                   />
                            <Label Grid.Row="5"
                                   Width="130" 
                                   HorizontalAlignment="Right" VerticalAlignment="Stretch" 
                                   Foreground="White" 
                                   FontSize="14"
                                   Content="{Binding CZ2Axis.Status.RawPosition.Actual}" 
                                   ContentStringFormat="F2"
                                   />

                        </Grid>
                    </Grid>
                    <Grid Grid.Column="1" Margin="10,0,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="2*"/>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="2*"/>
                        </Grid.RowDefinitions>

                        <Button Grid.Column="0" Grid.Row="0" Content=" △ 1 CAM X+ " FontSize="15"  Foreground="White"
                                Command="{Binding CX1PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Grid.ColumnSpan="2" Margin="1,0,148,0">

                        </Button>
                        <TextBox Grid.Column="0" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding CX1TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding CX1TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="0" Grid.Row="2" Content=" ▽ 1 CAM X- " FontSize="15"  Foreground="White"
                                Command="{Binding CX1NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Grid.RowSpan="2">

                        </Button>
                        <Button Grid.Column="1" Grid.Row="0" Content=" △ 1 CAM Y+ " FontSize="15"  Foreground="White"
                                Command="{Binding CY1PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Margin="1,0,0,0"/>
                        <TextBox Grid.Column="1" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding CY1TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding CY1TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="1" Grid.Row="2" Content=" ▽ 1 CAM Y- " FontSize="15"  Foreground="White"
                                Command="{Binding CY1NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Grid.RowSpan="2">

                        </Button>
                        <Button Grid.Column="2" Grid.Row="0" Content=" △ 1 CAM Z+ " FontSize="15"  Foreground="White"
                                Command="{Binding CZ1PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}">

                        </Button>
                        <TextBox Grid.Column="2" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding CZ1TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding CZ1TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="2" Grid.Row="2" Content=" ▽ 1 CAM Z- " FontSize="15"  Foreground="White"
                                Command="{Binding CZ1NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Grid.RowSpan="2">

                        </Button>
                        <Button Grid.Column="3" Grid.Row="0" Content=" △ 2 CAM X+ " FontSize="15"  Foreground="White"
                                Command="{Binding CX2PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}"/>
                        <TextBox Grid.Column="3" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding CX2TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding CX2TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="3" Grid.Row="2" Content=" ▽ 2 CAM X- " FontSize="15"  Foreground="White"
                                Command="{Binding CX2NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Grid.RowSpan="2">

                        </Button>
                        <Button Grid.Column="4" Grid.Row="0" Content=" △ 2 CAM Y+ " FontSize="15"  Foreground="White"
                                Command="{Binding CY2PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}"/>
                        <TextBox Grid.Column="4" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding CY2TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding CY2TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="4" Grid.Row="2" Content=" ▽2 CAM Y- " FontSize="15"  Foreground="White"
                                Command="{Binding CY2NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Grid.RowSpan="2">

                        </Button>
                        <Button Grid.Column="5" Grid.Row="0" Content=" △ 2 CAM Z+ " FontSize="15"  Foreground="White"
                                Command="{Binding CZ2PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" HorizontalAlignment="Right" Width="139" Margin="0,0,10,0"/>
                        <TextBox Grid.Column="5" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding CZ2TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding CZ2TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="5" Grid.Row="2" Content=" ▽ 2 CAM Z- " FontSize="15"  Foreground="White"
                                Command="{Binding CZ2NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Grid.RowSpan="2" Margin="0,0,10,0"/>
                    </Grid>
                </Grid>
            </GroupBox>
            <GroupBox Grid.Row="1" BorderBrush="White" BorderThickness="1" Margin="1" FontSize="18" Foreground="White" Header="Loader Jog" Grid.ColumnSpan="2">
                <GroupBox.Style>
                    <Style BasedOn="{x:Null}"/>
                </GroupBox.Style>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1.2*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Margin="10,0,10,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="3*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Label Content="A :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Label Content="U1 :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Label Content="U2 :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Label Content="W :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="4">
                                <Label Content="V :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid Grid.Row="5">
                                <Label Content="SC :" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Label Grid.Row="0" Content="{Binding AAxis.Status.RawPosition.Actual}" Foreground="White" Width="130" HorizontalAlignment="Right" VerticalAlignment="Stretch" FontSize="12"/>
                            <Label Grid.Row="1" Content="{Binding U1Axis.Status.RawPosition.Actual}" Foreground="White" Width="130" HorizontalAlignment="Right" VerticalAlignment="Stretch" FontSize="12"/>
                            <Label Grid.Row="2" Content="{Binding U2Axis.Status.RawPosition.Actual}" Foreground="White" Width="130" HorizontalAlignment="Right" VerticalAlignment="Stretch" FontSize="12"/>
                            <Label Grid.Row="3" Content="{Binding WAxis.Status.RawPosition.Actual}" Foreground="White" Width="130" HorizontalAlignment="Right" VerticalAlignment="Stretch" FontSize="12"/>
                            <Label Grid.Row="4" Content="{Binding VAxis.Status.RawPosition.Actual}" Foreground="White" Width="130" HorizontalAlignment="Right" VerticalAlignment="Stretch" FontSize="12"/>
                            <Label Grid.Row="5" Content="{Binding SCAxis.Status.RawPosition.Actual}" Foreground="White" Width="130" HorizontalAlignment="Right" VerticalAlignment="Stretch" FontSize="12"/>

                        </Grid>
                    </Grid>
                    <Grid Grid.Column="1" Margin="10,0,10,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="2*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="2*"/>
                        </Grid.RowDefinitions>

                        <Button Grid.Column="0" Grid.Row="0" Content=" △ A+ " FontSize="15"  Foreground="White"
                                Command="{Binding APosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}"/>
                        <TextBox Grid.Column="0" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding ATextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="0" Grid.Row="2" Content=" ▽ A- " FontSize="15"  Foreground="White"
                                Command="{Binding ANegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}">

                        </Button>
                        <Button Grid.Column="1" Grid.Row="0" Content=" △ U1+ " FontSize="15"  Foreground="White"
                                Command="{Binding U1PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}" Margin="0,0,1,0"/>
                        <TextBox Grid.Column="1" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding U1TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="1" Grid.Row="2" Content=" ▽ U1- " FontSize="15"  Foreground="White"
                                Command="{Binding U1NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}">

                        </Button>
                        <Button Grid.Column="2" Grid.Row="0" Content=" △ U2+ " FontSize="15"  Foreground="White"
                                Command="{Binding U2PosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}"/>
                        <TextBox Grid.Column="2" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding U2TextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="2" Grid.Row="2" Content=" ▽ U2- " FontSize="15"  Foreground="White"
                                Command="{Binding U2NegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}">

                        </Button>
                        <Button Grid.Column="3" Grid.Row="0" Content=" △ W+ " FontSize="15"  Foreground="White"
                                Command="{Binding WPosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}"/>
                        <TextBox Grid.Column="3" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding WTextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="3" Grid.Row="2" Content=" ▽ W- " FontSize="15"  Foreground="White"
                                Command="{Binding WNegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}">

                        </Button>
                        <Button Grid.Column="4" Grid.Row="0" Content=" △ V+ " FontSize="15"  Foreground="White"
                                Command="{Binding VPosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}"/>
                        <TextBox Grid.Column="4" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding VTextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="4" Grid.Row="2" Content=" ▽ V- " FontSize="15"  Foreground="White"
                                Command="{Binding VNegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}">

                        </Button>
                        <Button Grid.Column="5" Grid.Row="0" Content=" △ SC+ " FontSize="15"  Foreground="White"
                                Command="{Binding ScPosMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}"/>
                        <TextBox Grid.Column="5" Grid.Row="1" Width="70" Height="40" Margin="0,0,0,0" FontSize="20"                             
                             Text="{Binding ScTextVal}"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDown" >
                                    <i:InvokeCommandAction Command="{Binding TextBoxClickCommand}"
                                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TextBox}}}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </TextBox>
                        <Button Grid.Column="5" Grid.Row="2" Content=" ▽ SC- " FontSize="15"  Foreground="White"
                                Command="{Binding ScNegMoveCommand}" IsEnabled="{Binding StageButtonsVisibility}">

                        </Button>
                    </Grid>
                </Grid>

            </GroupBox>
            <!--GroupBox Grid.Row="7" BorderBrush="White" BorderThickness="1" Margin="10" FontSize="18" Foreground="White" Header="Chuck X/Y Position Change" Grid.RowSpan="2">
                <GroupBox.Style>
                    <Style BasedOn="{x:Null}"/>
                </GroupBox.Style>
                <Grid Margin="10,0,10,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Button Grid.Column="0" Grid.Row="0" Command="{Binding MoveToBackCommand}">
                        <Label Content="        ▲&#10;Move To Back" FontSize="25" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

                    </Button>
                    <Button Grid.Column="1" Grid.Row="1" Command="{Binding MoveToCenterCommand}" >
                        <Label Content="Move To Center" FontSize="23" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

                    </Button>
                    <Button Grid.Column="2" Grid.Row="2" Command="{Binding MoveToFrontCommand}" >
                        <Label Content="Move To Front&#10;        ▼" FontSize="25" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

